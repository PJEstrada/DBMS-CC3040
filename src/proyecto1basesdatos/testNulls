INSERT INTO A VALUES (18,3);
// Al hacer update a un valor null, deberia tirar error por la primary key

UPDATE films SET code = NULL WHERE tittle = 'myMovie'


UPDATE A SET idSerial = NULL WHERE estudiante = 3