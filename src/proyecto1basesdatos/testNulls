CREATE TABLE films
(
code INT,
tittle CHAR(255),
release_date DATE,
kind CHAR(255),
duracion INT,

CONSTRAINT pkeyfilms PRIMARY KEY (code)

)


CREATE TABLE A
(
  idSerial INT,
  estudiante INT,

  CONSTRAINT pkeyA PRIMARY KEY (idSerial),
  CONSTRAINT fkeyAsignacion FOREIGN KEY (idSerial) REFERENCES films (code)
)

INSERT INTO films VALUES (13,'myMovie','1999-03-03','Ficcion',80); 
