CREATE TABLE asignacion
(
  idserial INT,
  estudianteid INT,
  universidadid INT,
  materia CHAR(255),
  nota INT,
  CONSTRAINT pkeyAsignacion PRIMARY KEY (idserial)
)

CREATE TABLE A
(
  idSerial INT,
  estudiante INT,
  CONSTRAINT pkeyAsignacion FOREIGN KEY (idSerial,estudiante) REFERENCES asignacion (idserial,estudianteid)
)