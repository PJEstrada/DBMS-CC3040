CREATE TABLE asignacion
(
  idserial INT,
  estudianteid INT,
  universidadid INT,
  materia CHAR(255),
  nota INT,
  CONSTRAINT pkeyAsignacion PRIMARY KEY (idserial)
  CONSTRAINT pkeyAsignacion2 PRIMARY KEY (estudianteid)
  CONSTRAINT checkNota CHECK ( 7>8 AND (nota<100) )
)

CREATE TABLE A
(
  idSerial INT,
  estudiante INT,

  CONSTRAINT pkeyA PRIMARY KEY (idSerial)
  CONSTRAINT fkeyAsignacion FOREIGN KEY (idSerial,estudiante) REFERENCES asignacion (idserial,estudianteid)
)

CREATE TABLE films
(
code INT,
tittle CHAR(255),
release_date DATE,
kind CHAR(255),

CONSTRAINT pkeyfilms PRIMARY KEY (code)

)

ALTER TABLE asignacion RENAME TO asignacion2