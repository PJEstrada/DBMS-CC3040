/*Pruebas*/
CREATE TABLE films
(
  tittle CHAR(255),
  release_date DATE,
  kind CHAR(255),
  duracion INT,
  newcol1 INT,
  CONSTRAINT films_tittle_fkey FOREIGN KEY (tittle)
      REFERENCES asignacion (materia)
    
)


CREATE TABLE asignacion
(
  idserial INT,
  estudianteid INT,
  universidadid INT,
  materia CHAR(255) ,
  nota INT,
  CONSTRAINT asignacion_pkey PRIMARY KEY (materia)
)