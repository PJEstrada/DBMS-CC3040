CREATE TABLE asignacion
(
  idserial INT,
  estudianteid INT,
  universidadid INT,
  materia CHAR(255),
  nota INT,
  fecha_ingreso DATE,
  CONSTRAINT pkeyAsignacion PRIMARY KEY (idserial,estudianteid),
  CONSTRAINT checkNota CHECK (nota<10 )
)

CREATE TABLE A 
(
  idSerialA INT,
  estudiante INT,

  CONSTRAINT pkeyA PRIMARY KEY (idSerialA),
  CONSTRAINT fkeyAsignacion FOREIGN KEY (idSeriala,estudiante) REFERENCES asignacion (idserial,estudianteid)
)


